/**
 * Adds two 16-bit values.
 * The most significant carry bit is ignored.
 */

CHIP Add32 {
    IN ip1[32], ip2[32];
    OUT out[32],carry;

    PARTS:
   // Put you code here:
   And16(ip1[0..15]=ip1[0..16],ip2[0..15]=ip2[0..15],out[0..15]=out[0..15],carry=c1);
   
}